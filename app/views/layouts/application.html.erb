<!DOCTYPE html>
<html>
  <head>
    <title><%= t("name")%></title>
    <%= stylesheet_link_tag :all %>
    <%= javascript_include_tag "jquery-1.5.min" %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <%= render :partial => "partials/navigate"%>
    <div id="wrap">
      <div id="content">
        <%= yield %>
      </div>
      <div id="footer" class="clearfix">
        <div id="copyright">
          <%= t("copyright", :year => Time.new.year)%>
        </div>
      </div>
    </div>
  </body>
</html>
